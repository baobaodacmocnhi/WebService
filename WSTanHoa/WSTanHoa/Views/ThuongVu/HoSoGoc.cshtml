@model WSTanHoa.Models.MView
@{
    Layout = null;
    ViewBag.Title = "Hồ Sơ Góc";
}

<head>
    <link rel="shortcut icon" type="image/ico" href="~/Images/logoctycp.png">
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/cssNOTMASTER")
</head>
<body>
    <div class="bd-example border rounded-3 shadow-sm">
        <div class="overflow-auto">
            <table class="table table-striped table-bordered text-center">
                <thead>
                    <tr style="background-color:#00b3b3;color:white">
                        <th class="col-md-1">STT</th>
                        <th class="col-md-1">Thời Gian</th>
                        <th class="col-md-1">File</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model.lst)
                    {
                        <tr>
                            <td>
                                @item.SoLuong
                            </td>
                            <td>
                                @item.ThoiGian
                            </td>
                            <td>
                                <a href="http://service.cskhtanhoa.com.vn/ThuongVu/HoSoGoc_viewFilePDF?path=@item.NoiDung" target="_blank">Xem File</a>
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>

</body>
